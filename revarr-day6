#include<iostream>
using namespace std;
int arr(int arr[],int n){
    int s=0;
    int e=n-1;
    if(s<=e){
        swap(arr[s++],arr[n--])
    }
}
void reversearr(int arr[],int n){
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }cout<<endl;
}
int main(){
    int array[5]={1,2,3,4,5};
    arr(arr,n);
    return 0;
    
}
