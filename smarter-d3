#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    double l,v1,v2;
	    cin>>l>>v1>>v2;
	    double t,h;
	    t=ceil(l/v1);
	    h=ceil(l/v2);
	    if(t==h){
	        cout<<-1<<endl;
	    }else{
	        cout<<t-h-1<<endl;
	    }
	    
	}
	return 0;
}
