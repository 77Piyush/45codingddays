#include<iostream>
using namespace std;
int main(){
int arr[6]={1,8,8,1,2,3};
int largest= arr[0];
for(int i=1;i<6;i++){
    if(arr[i]>largest){
        largest=arr[i];
    }
}
 int secondlar=-1;
for(int i=0;i<6;i++){
if(arr[i]!=largest && arr[i]>secondlar){
    secondlar=arr[i];
}

}
cout<<" the second largest is : "<<secondlar;


}
